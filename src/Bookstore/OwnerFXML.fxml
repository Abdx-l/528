<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" 
            xmlns:fx="http://javafx.com/fxml/1" 
            xmlns="http://javafx.com/javafx/22" 
            fx:controller="Bookstore.OwnerFXMLController">
   <children>
      <Button fx:id="booksButton" layoutX="158.0" layoutY="96.0" 
              mnemonicParsing="false" prefHeight="25.0" prefWidth="285.0" 
              text="Books" onAction="#handleBooksButton" />
      
      <Button fx:id="customersButton" layoutX="158.0" layoutY="175.0" 
              mnemonicParsing="false" prefHeight="25.0" prefWidth="285.0" 
              text="Customers" onAction="#handleCustomersButton" />
      
      <Button fx:id="logoutButton" layoutX="158.0" layoutY="251.0" 
              mnemonicParsing="false" prefHeight="25.0" prefWidth="285.0" 
              text="Logout" onAction="#handleLogoutButton" />
   </children>
</AnchorPane>